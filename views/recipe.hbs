<h1>{{recipe.name}}</h1>

<div class="user-info" {{!-- class="col s12 m8 offset-m2 l6 offset-l3" --}}>
  <div class="card-panel grey lighten-5 z-depth-1 user-info__row">
    <div class="row valign-wrapper user-info__row__card">
      <div class="col s2">
        <img img src="{{this.diffuse}}" alt="user profile image" onerror="this.src='/images/user.jpg';"
          class="circle responsive-img">
      </div>
      <div class="col s10">
        <span class="black-text">
          <p class="user-info__row__card__text">Created by: {{recipe.creator.username}}</p>
          <p class="user-info__row__card__text">Created by: {{recipe.creator.email}}</p>
        </span>
      </div>
    </div>
  </div>
</div>

